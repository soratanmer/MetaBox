{
    "mode": "rule",
    "mixed-port": 51300,
    "allow-lan": true,
    "log-level": "info",
    "ipv6": true,
    "secret": "accpetWYN",
    "external-controller": "127.0.0.1:51225",
    "find-process-mode": "strict",
    "global-client-fingerprint": "chrome",
    "profile": {
        "store-selected": true,
        "store-fake-ip": true
    },
    "sniffer": {
        "enable": true,
        "parse-pure-ip": true,
        "sniff": {
            "HTTP": {
                "ports": [
                    "80",
                    "8080-8880"
                ]
            },
            "TLS": {
                "ports": [
                    "443",
                    "8443"
                ]
            },
            "QUIC": {
                "ports": [
                    "443",
                    "8443"
                ]
            }
        },
        "skip-domain": [
            "Mijia Cloud"
        ]
    },
    "dns": {
        "enable": true,
        "ipv6": true,
        "prefer-h3": true,
        "respect-rules": true,
        "enhanced-mode": "fake-ip",
        "fake-ip-range": "11.0.0.1/16",
        "fake-ip-filter": [
            "*",
            "+.lan",
            "+.local",
            "+.market.xiaomi.com"
        ],
        "nameserver": [
            "https://8.8.8.8/dns-query",
            "https://1.1.1.1/dns-query"
        ],
        "proxy-server-nameserver": [
            "https://223.5.5.5/dns-query",
            "https://120.53.53.53/dns-query"
        ],
        "nameserver-policy": {
            "rule-set:domain-geolocation-!cn": [
                "https://8.8.8.8/dns-query",
                "https://1.1.1.1/dns-query"
            ],
            "rule-set:domain-cn": [
                "https://223.5.5.5/dns-query",
                "https://120.53.53.53/dns-query"
            ],
            "rule-set:domain-ads": [
                "rcode://success"
            ]
        }
    },
    "tun": {
        "enable": true,
        "auto-route": true,
        "auto-detect-interface": true,
        "strict-route": true,
        "stack": "system",
        "device": "Meta",
        "dns-hijack": [
            "any:53",
            "tcp://any:53"
        ]
    },
    "proxy-providers": {},
